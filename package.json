{
  "name": "cypress-bases",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "env": {
    "NO_PROXY": "localhost.proxy.com"
  },
  "scripts": {
    "cy:open": "cypress open",
    "cy:run": "cypress run",
